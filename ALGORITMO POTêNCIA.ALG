Algoritmo "Algoritmo potência"

Var
   num, num2, potencia : real

Inicio
      Escreval("Insira a base da potenciação: ")
      Leia (num)
      Escreval
      Escreval("Insira o expoente da potenciação: ")
      Leia (num2)
      Escreval
      potencia <- num ^ num2
      Escreval(num, " ^", num2, " é igual a: ", potencia)
      Escreval
   
Fimalgoritmo